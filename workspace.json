{
  "version": 1,
  "projects": {
    "fantasy-empires": {
      "root": "apps/fantasy-empires",
      "sourceRoot": "apps/fantasy-empires/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/fantasy-empires",
            "index": "apps/fantasy-empires/src/index.html",
            "main": "apps/fantasy-empires/src/main.ts",
            "polyfills": "apps/fantasy-empires/src/polyfills.ts",
            "tsConfig": "apps/fantasy-empires/tsconfig.app.json",
            "assets": [
              "apps/fantasy-empires/src/favicon.ico",
              "apps/fantasy-empires/src/assets"
            ],
            "styles": ["apps/fantasy-empires/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/fantasy-empires/src/environments/environment.ts",
                  "with": "apps/fantasy-empires/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "fantasy-empires:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "fantasy-empires:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/fantasy-empires/tsconfig.app.json",
              "apps/fantasy-empires/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/fantasy-empires/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/fantasy-empires/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/fantasy-empires"]
        }
      }
    },
    "fantasy-empires-e2e": {
      "root": "apps/fantasy-empires-e2e",
      "sourceRoot": "apps/fantasy-empires-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/fantasy-empires-e2e/cypress.json",
            "tsConfig": "apps/fantasy-empires-e2e/tsconfig.e2e.json",
            "devServerTarget": "fantasy-empires:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fantasy-empires:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/fantasy-empires-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/fantasy-empires-e2e/**/*"]
          }
        }
      }
    },
    "house-wars": {
      "projectType": "application",
      "root": "apps/house-wars",
      "sourceRoot": "apps/house-wars/src",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/house-wars",
            "index": "apps/house-wars/src/index.html",
            "main": "apps/house-wars/src/main.ts",
            "polyfills": "apps/house-wars/src/polyfills.ts",
            "tsConfig": "apps/house-wars/tsconfig.app.json",
            "assets": [
              "apps/house-wars/src/favicon.ico",
              "apps/house-wars/src/assets"
            ],
            "styles": ["apps/house-wars/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/house-wars/src/environments/environment.ts",
                  "with": "apps/house-wars/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
